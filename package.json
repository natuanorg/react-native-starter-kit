{
  "name": "ReactNativeStarterKit",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "rename": "node ./bin/rename.js",
    "clear": "watchman watch-del-all && rm -rf $TMPDIR/react-native-packager-cache-* && rm -rf $TMPDIR/metro-bundler-cache-* && rm -rf node_modules/ && npm cache clean --force && npm install && npm start -- --reset-cache",
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "test": "jest",
    "iosx": "react-native run-ios --simulator='iPhone X'",
    "ios": "react-native run-ios --simulator='iPhone 6'",
    "android": "react-native run-android --appId vn.react.native.starter.kit"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "lodash": "^4.17.10",
    "react": "16.6.3",
    "react-native": "0.58.4",
    "react-native-firebase": "^5.2.2",
    "react-native-gesture-handler": "^1.0.10",
    "react-native-loading-spinner-overlay": "^0.5.2",
    "react-native-modal-datetime-picker": "^6.0.0",
    "react-native-vector-icons": "^6.3.0",
    "react-navigation": "^3.0.0",
    "react-redux": "^5.0.7",
    "redux": "^4.0.0",
    "redux-actions": "^2.6.1",
    "redux-logger": "^3.0.6",
    "redux-persist": "^5.10.0",
    "redux-thunk": "^2.3.0"
  },
  "devDependencies": {
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "24.1.0",
    "fs-extra": "^6.0.1",
    "jest": "24.1.0",
    "metro-react-native-babel-preset": "0.51.1",
    "react-test-renderer": "16.6.3",
    "replace-in-file": "^3.4.0"
  },
  "jest": {
    "preset": "react-native"
  }
}
